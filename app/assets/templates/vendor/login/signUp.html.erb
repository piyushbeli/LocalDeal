<!-- New -->
<h1><i class="fa fa-pencil"></i><span>Sign Up Form</span></h1>
<form class="register-form" name="signUpForm" role="form" ng-submit="doRegister()" valdr-type="SignUp" novalidate>
  <div class="form-field"> <span class="fa fa-user"></span>
    <input type="text" class="form-control" placeholder="e.g Macdonalds" ng-model="vendor.name" name="name">
  </div>
  <div class="form-field"> <span class="fa fa-envelope-o"></span>
    <input type="text" class="form-control" placeholder="e.g piyush.beli@gmail.com" ng-model="vendor.email" name="email">
  </div>
  <div class="form-field"> <span class="fa fa-envelope-o"></span>
    <input type="string" class="form-control" placeholder="e.g 9928618166" ng-model="vendor.mobile" name="mobile">
  </div>
  <div class="form-field"> <span class="fa fa-unlock"></span>
    <input type="password" class="form-control" placeholder="password" ng-model="vendor.password" name="password">
  </div>
  <div class="form-field"> <span class="fa fa-unlock"></span>
    <input type="password" class="form-control" placeholder="retype password" ng-model="vendor.confirm_password" name="confirmPassword">
  </div>
  <div class="form-field">
    <button type="submit">Sign Up</button>
  </div>
  <div class="register-reset">
    <div class="right-sec">
      <a ng-click="showLoginForm = true" href="javascript:void(0)">Sign in</a>
    </div>
  </div>
</form>