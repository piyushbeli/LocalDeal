<div class="container-fluid">
  <h3>Verify Mobile No</h3>
  <br>
  <h4>Mobile no where otp will be sent:</h4>
  <input disabled name="mobileNo" placeholder="Mobile No" ng-model="vendor.mobile">
  <button class="btn btn-info" ng-click="sendOTP()">Send OTP</button>
  <br>
  <span>Please enter the OTP you received.</span>
  <div class="row">
    <div class="col-lg-6">
      <form role="form" name="verifyMeForm" ng-submit="verifyMe()">
        <div class="form-group">
          <input class="form-control" type="text" ng-model="otp">
        </div>
        <button class="btn btn-danger" type="submit">Submit</button>
      </form>
    </div>
  </div>
  <button class="btn btn-warning" ng-click="skipVerification()">Skip Verification</button>
</div>


