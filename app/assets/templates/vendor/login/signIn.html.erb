<!-- *********New Login Form -->





<h1><i class="fa fa-lock"></i><span>Login Form</span></h1>
<form class="login-form" name="signInForm" ng-submit="doLogin()" novalidate>>
  <div class="form-field"> <span class="fa fa-user"></span>
    <input type="text" class="form-control" placeholder="username" ng-model="vendor.email" name="userName">
  </div>
  <div class="form-field"> <span class="fa fa-unlock"></span>
    <input type="password" class="form-control" placeholder="password" ng-model="vendor.password" name="password">
  </div>
  <div class="form-field">
    <button type="submit">Login</button>
  </div>
  <div class="login-reset">
    <div class="left-sec"> <a href="#">Forgot the password ?</a> </div>
    <div class="right-sec"><a ng-click="showLoginForm = false" href="javascript:void(0)">Sign up</a> </div>
  </div>
</form>