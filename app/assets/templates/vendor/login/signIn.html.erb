<form role="form" ng-submit="doLogin()">
  <div class="form-group">
    <input type="text" class="form-control" placeholder="username" ng-model="vendor.email" required>
  </div>
  <div class="form-group">
    <input type="password" class="form-control" placeholder="password" ng-model="vendor.password" required>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit">Login</button>
  </div>
</form>