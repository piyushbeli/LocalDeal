<div class="row">
  <div class="col col-md-6">
    <form class="form" role="form" ng-submit="updateProfile()">
      <div class="form-group">
        <input class="form-control" type="text" ng-model="vendor.website" placeholder="website link">
      </div>
      <div class="form-group">
        <input class="form-control" type="text" ng-model="vendor.mobile" placeholder="mobile #">
      </div>
      <div class="form-group">
        <label class="control-label">Category</label>
        <select class="form-control" ng-model="vendor.category"
                ng-options="category.name for category in data.categories"></select>
      </div>
      <div class="form-group">
        <label class="control-label">Sub Category</label>
        <select class="form-control" ng-model="vendor.subcategories" multiple
                ng-options="subcategory.name for subcategory in vendor.category.subcategories"></select>
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">Update</button>
      </div>
    </form>
  </div>
  <div class="col col-md-5">
    <div outlet-form></div>
  </div>
</div>
<div class="row">
  <a ui-sref="app.deals">show deals</a>
</div>
