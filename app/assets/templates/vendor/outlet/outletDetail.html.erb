<!-- new -->
<article>
  <section class="video-container">
    <iframe width="853" height="510" frameborder="0" allowfullscreen="" src="http://www.youtube.com/embed/3R2cnxz27LI"></iframe>
  </section>
  <div class="container">
    <div class="row">
      <section class="inner-container">
        <div class="col-lg-6 col-sm-6">
          <div class="loc-outer">
            <h2>
                <span>
                    <i class="fa fa-map-marker"></i>
                </span>
              Locations
            </h2>
            <form class="out-det-form">
              <div class="form-group">
                <label>City</label>
                <input id="searchAddress" ng-autocomplete ng-model="outlet.city"
                       details="outlet.cityDetail" options="googlePlaceAutoCompleteOptionsCity" autocomplete="off">
              </div>
              <div class="form-group">
                <label>Street</label>
                <input id="searchAddress" ng-autocomplete ng-model="outlet.street"
                       details="outlet.streetDetail" options="googlePlaceAutoCompleteOptionsStreet" autocomplete="off">
              </div>
              <div class="form-group">
                <label>Address</label>
                <input type="text" ng-model="outlet.address">
              </div>
            </form>
          </div>
        </div>
        <div class="col-lg-6 col-sm-6">
          <div class="loc-outer mr-btm-null">
            <h2>
                <span>
                    <i class="fa fa-mobile"></i>
                </span>
              Contact Information
            </h2>
            <form class="out-det-form">
              <div class="form-group">
                <label>Mobile No.</label>
                <input type="number" ng-model="outlet.mobile" maxlength="10" a>
              </div>
              <div class="form-group">
                <label>Landline No.</label>
                <input type="number" ng-model="outlet.contactNo">
              </div>
              <div class="form-group">
                <label>Email- ID</label>
                <input type="email" ng-model="outlet.email">
              </div>
            </form>
          </div>
        </div>

        <div class="clearfix"></div>

        <div class="col-lg-12">
          <div class="gen-btn out-loca">
            <a href="javascript:void(0)" ng-click="save()">Update</a>
            <a href="javascript:void(0)" ng-click="close()">Cancel</a>
          </div>
        </div>

        <div class="clearfix"></div>

        <div class="col-lg-12">
          <div class="map-cont">
            <div class="pin-me-map">
                <span>
                  <label class="checkbox" style="margin-left: 20px">
                    <input type="checkbox"  ng-model="shouldUseCurrentLocation" ng-change="userCurrentLocation()">
                    Use current location to pin on map (If you are currently sitting in your outlet)
                  </label>
                <strong>Note:</strong> We tried to pin your location on the map based on information provided, please move the marker to adjust the location
                </span>
              <a href="#">Pin me</a>
            </div>
            <div class="map-inner">
              <ui-gmap-google-map center="map.center" zoom="15" draggable="true" options="options">
                <ui-gmap-marker fit="true" coords="marker.coords" options="marker.options" events="marker.events" idkey="marker.id">
                </ui-gmap-marker>
              </ui-gmap-google-map>
            </div>
          </div>
        </div>
      </section>
    </div>
  </div>
</article>
