


<!-- new -->
<article>
  <!--<section class="video-container">
    <iframe width="853" height="510" frameborder="0" allowfullscreen="" src="http://www.youtube.com/embed/3R2cnxz27LI"></iframe>
  </section>-->

  <section class="profile-banner" ng-if="!outlet.isNew()">
    <div class="profile-wrapper">
      <div class="container">
        <div class="profile-inner">
          <div class="profile-pic">
            <a href="#" ngf-select="uploadProfilePic($files)" multiple accept="image/*" ngf-max-size="7MB" ngf-resize="{width: 200, height: 200}">
              <img src="images/profile-pic.png" alt="profile-pic">
            </a>
          </div>
          <div class="pro-counters">
            <ul class="list-inline">
              <li>
                <h2>Followers<span>{{outlet.noOfFollowers || 0}}</span></h2>
              </li>
              <li>
                <h2>Reviews<span>{{outlet.noOfReviews || 0}}</span></h2>
              </li>
              <li>
                <h2>
                  {{outlet.averageRating || 'NA'}} <span>{{outlet.noOfRaters || 0}} Votes</span>
                  <!--<br>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star-half-empty"></i>
                  <i class="fa fa-star-o"></i>-->
                </h2>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </section>

  <div class="container">
    <div class="row">
      <section class="inner-container">
        <div class="col-lg-6 col-sm-6">
          <div class="loc-outer mr-btm-null">
            <h2>
                <span>
                    <i class="fa fa-mobile"></i>
                </span>
              Contact Information
            </h2>
            <form class="out-det-form" name="outletContactDetailForm" role="form" valdr-type="Outlet">
              <div valdr-form-group class="form-group">
                <label>Name</label>
                <input ng-model="outlet.name" name="name">
              </div>
              <div valdr-form-group class="form-group">
                <label>Mobile No.</label>
                <input type="text" ng-model="outlet.mobile" maxlength="10" name="mobile">
              </div>
              <div valdr-form-group class="form-group">
                <label>Landline No.</label>
                <input type="text" ng-model="outlet.contactNo" name="contactNo">
              </div>
              <div valdr-form-group class="form-group">
                <label>Email- ID</label>
                <input type="email" ng-model="outlet.email" name="email">
              </div>
            </form>
          </div>
        </div>
        <div class="col-lg-6 col-sm-6">
          <div class="loc-outer">
            <h2>
                <span>
                    <i class="fa fa-map-marker"></i>
                </span>
              Locations
            </h2>
            <form class="out-det-form" name="outletLocationDetailForm" role="form" valdr-type="Outlet">
              <div valdr-form-group class="form-group">
                <label>City</label>
                <input id="searchAddress" ng-autocomplete ng-model="outlet.city"
                       details="outlet.cityDetail" options="googlePlaceAutoCompleteOptionsCity" autocomplete="off" name="city">
              </div>
              <div valdr-form-group class="form-group">
                <label>Location</label>
                <input id="searchAddress" ng-autocomplete ng-model="outlet.street"
                       details="outlet.streetDetail" options="googlePlaceAutoCompleteOptionsStreet"  autocomplete="off" name="street">
              </div>
              <div valdr-form-group class="form-group">
                <label>Address</label>
                <textarea ng-model="outlet.address" rows="4"></textarea>
              </div>

            </form>
          </div>
        </div>

        <div class="clearfix"></div>

        <div class="col-lg-12">
          <div class="gen-btn out-loca">
            <a href="javascript:void(0)" ng-click="save()">Update</a>
            <a href="javascript:void(0)" ng-click="cancel()">Cancel</a>
          </div>
        </div>

        <div class="clearfix"></div>

        <div class="col-lg-12">
          <div class="map-cont">
            <div class="pin-me-map">
                <span>
                  <label class="checkbox" style="margin-left: 20px">
                    <input type="checkbox"  ng-model="shouldUseCurrentLocation" ng-change="userCurrentLocation()">
                    Use current location to pin on map (If you are currently sitting in your outlet)
                  </label>
                <strong>Note:</strong> We tried to pin your location on the map based on information provided, please move the marker to adjust the location
                </span>
              <a href="javascript:" ng-click="pinOutlet()">Pin me</a>
            </div>
            <div class="map-inner">
              <ui-gmap-google-map center="map.center" zoom="map.zoom" draggable="true" options="map.options" control="{}" events="map.events">
                <<!--ui-gmap-marker  coords="map.center" options="marker.options" events="marker.events" idkey="marker.id">
                </ui-gmap-marker>-->
              </ui-gmap-google-map>
            </div>
          </div>
        </div>

        <div class="photos col-lg-12">
          <div class="row">
            <h3>Photos:</h3>
            <hr>
            <div class="gen-btn add-photo">
              <a ngf-select="uploadImages($files)" multiple accept="image/*" ngf-max-size="7MB" ngf-resize="{width: 200, height: 200}">Add photo</a>
            </div>
          </div>

          <div class="row">
            <div ng-repeat="image in outlet.images" class="col-lg-4">
              <img ng-src="{{image.url}}" alt="{{outlet.name + image.caption}}" class="img-thumbnail">
              <div class="caption">
                <p>{{caption}}</p>
              </div>
            </div>
          </div>
          <div class="text-center">
            <img src="images/loading.gif" ng-show="isLoadingImages">
          </div>
        </div>

        <!-- Menus -->
        <div class="menus col-lg-12">
          <div class="row">
            <h3>Menus:</h3>
            <hr>
            <div class="gen-btn add-photo">
              <a  ngf-select="uploadMenus($files)" multiple accept="image/*" ngf-max-size="7MB" ngf-resize="{width: 200, height: 200}">Add menu</a>
            </div>
          </div>

          <div class="row">
            <div ng-repeat="menu in outlet.menus" class="col-lg-4">
                <img ng-src="{{menu.url}}" alt="{{outlet.name + image.caption}}" class="img-thumbnail">
                <div class="caption">
                  <p>{{caption}}</p>
                </div>
            </div>
          </div>
          <div class="text-center">
            <img src="images/loading.gif" ng-show="isLoadingMenus">
          </div>
        </div>

      </section>
    </div>
  </div>
</article>
