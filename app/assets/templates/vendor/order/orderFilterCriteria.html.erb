<div class="row">
  <div class="col-lg-6 col-md-6 col-sm-12">
    <div class="order_sorting">
      <div class="order-filter-inner">
        <h5>Filter</h5>
        <div class="btn-group" uib-dropdown uib-keyboard-nav>
          <button id="simple-btn-keyboard-nav" type="button" class="btn btn-primary" uib-dropdown-toggle>
            Dropdown with keyboard navigation <span class="caret"></span>
          </button>
          <ul class="uib-dropdown-menu" role="menu" aria-labelledby="simple-btn-keyboard-nav">
            <li role="menuitem"><a href="#">Action</a></li>
            <li role="menuitem"><a href="#">Another action</a></li>
            <li role="menuitem"><a href="#">Something else here</a></li>
            <li class="divider"></li>
            <li role="menuitem"><a href="#">Separated link</a></li>
          </ul>
        </div>
      </div>
      <div class="order-filter-inner">
        <h5>State</h5>
        <div class="dropdown">
          <button data-toggle="dropdown" type="button" aria-expanded="false">Rajasthan
            <span class="caret"></span></button>
          <ul class="dropdown-menu">
            <li><a href="#">Delhi/NCR</a></li>
            <li><a href="#">Tamil Nadu</a></li>
            <li><a href="#">Punjab</a></li>
          </ul>
        </div>
      </div>
      <div class="order-filter-inner">
        <h5>Date</h5>
        <div class="dropdown">
          <button data-toggle="dropdown" type="button" aria-expanded="false">DD
            <span class="caret"></span></button>
          <ul class="dropdown-menu">
            <li><a href="#">10</a></li>
            <li><a href="#">11</a></li>
            <li><a href="#">12</a></li>
            <li><a href="#">13</a></li>
          </ul>
        </div>
        <div class="dropdown">
          <button data-toggle="dropdown" type="button" aria-expanded="false">MM
            <span class="caret"></span></button>
          <ul class="dropdown-menu">
            <li><a href="#">01</a></li>
            <li><a href="#">02</a></li>
            <li><a href="#">03</a></li>
            <li><a href="#">04</a></li>
          </ul>
        </div>
        <div class="dropdown">
          <button data-toggle="dropdown" type="button" aria-expanded="false">YYYY
            <span class="caret"></span></button>
          <ul class="dropdown-menu">
            <li><a href="#">1991</a></li>
            <li><a href="#">1992</a></li>
            <li><a href="#">1993</a></li>
            <li><a href="#">1994</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-6 col-md-6 col-sm-12">
    <div class="table-search">
      <input type="text" placeholder="Search here">
      <a href="#">Search</a>
    </div>
  </div>
</div>
